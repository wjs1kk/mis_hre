			return;
		 }
		 */
		// 유효성검사(일반항목)    항목명 : this.in_TRP_NM.text
		if (!this.gfn_chkValue(this.SBJ, [this.in_TRP_NM.text], "MAX_LEN:10|NULL")) {
			return false;
		}

		// 유효성검사(그리드)
